{
	"name": "azde_case_data",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "azde_dataflow_case_data",
						"type": "DatasetReference"
					},
					"name": "SampleSetCaseData"
				},
				{
					"dataset": {
						"referenceName": "azde_dataflow_country_lookup",
						"type": "DatasetReference"
					},
					"name": "SampleSetCountryLookup"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "azde_dataflow_sink_casedata",
						"type": "DatasetReference"
					},
					"name": "SinkCaseData"
				}
			],
			"transformations": [
				{
					"name": "SelectionCaseData"
				},
				{
					"name": "FilterCaseData"
				},
				{
					"name": "LookupCaseData"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as integer,",
				"          date as date,",
				"          rate_14_day as string,",
				"          source as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> SampleSetCaseData",
				"source(output(",
				"          country as string,",
				"          country_code_2_digit as string,",
				"          country_code_3_digit as string,",
				"          continent as string,",
				"          population as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> SampleSetCountryLookup",
				"SampleSetCaseData select(mapColumn(",
				"          country,",
				"          country_code,",
				"          continent_name = continent,",
				"          population,",
				"          daily_count,",
				"          date,",
				"          rate_14_day,",
				"          source",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectionCaseData",
				"SelectionCaseData filter(continent_name == 'Asia') ~> FilterCaseData",
				"FilterCaseData, SampleSetCountryLookup lookup(SelectionCaseData@country == SampleSetCountryLookup@country,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> LookupCaseData",
				"LookupCaseData sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1) ~> SinkCaseData"
			]
		}
	}
}